openapi: 3.0.0
info:
  description: Consulta e insere CEP
  version: 1.0.0
  title: cicscep
paths:
  "/consulta/{codigo}":
    get:
      tags:
        - cicscep
      operationId: getCicsCepSerCon
      parameters:
        - name: Authorization
          in: header
          required: false
          schema:
            type: string
        - name: codigo
          in: path
          required: true
          schema:
            type: string
            maxLength: 8
      responses:
        "200":
          description: OK
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/getCicsCepSerCon_response_200"
  "/insere/{codigo}/{uf}/{cidade}/{bairro}/{logradouro}":
    get:
      tags:
        - cicscep
      operationId: getCicsCepSerIns
      parameters:
        - name: Authorization
          in: header
          required: false
          schema:
            type: string
        - name: uf
          in: path
          required: true
          schema:
            type: string
            maxLength: 2
        - name: codigo
          in: path
          required: true
          schema:
            type: string
            maxLength: 8
        - name: cidade
          in: path
          required: true
          schema:
            type: string
            maxLength: 30
        - name: bairro
          in: path
          required: true
          schema:
            type: string
            maxLength: 30
        - name: logradouro
          in: path
          required: true
          schema:
            type: string
            maxLength: 30
      responses:
        "200":
          description: OK
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/getCicsCepSerIns_response_200"
servers:
  - url: https://localhost:8080/cicscep
  - url: http://localhost:8080/cicscep
components:
  schemas:
    getCicsCepSerCon_response_200:
      type: object
      properties:
        cep:
          type: object
          properties:
            codigo:
              type: string
              maxLength: 8
            uf:
              type: string
              maxLength: 2
            cidade:
              type: string
              maxLength: 30
            bairro:
              type: string
              maxLength: 30
            logradouro:
              type: string
              maxLength: 30
    getCicsCepSerIns_response_200:
      type: object
      properties:
        cep:
          type: object
          properties:
            codigo:
              type: string
              maxLength: 8
            uf:
              type: string
              maxLength: 2
            cidade:
              type: string
              maxLength: 30
            bairro:
              type: string
              maxLength: 30
            logradouro:
              type: string
              maxLength: 30